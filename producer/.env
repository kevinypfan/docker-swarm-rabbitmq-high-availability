# RabbitMQ 連接設定
RABBITMQ_URL=amqp://admin:test1234@localhost:5672
# RABBITMQ_HOSTS=amqp://admin:test1234@rabbitmq:5672
# RABBITMQ_HOSTS=amqp://admin:test1234@localhost:5672

# Exchange 設定
EXCHANGE_NAME=test-exchange
ROUTING_KEY=test.message

# Producer 設定
HOSTNAME=producer-${TASK_SLOT:-1}
PORT=3000

# 自動發送設定
AUTO_SEND=true
AUTO_SEND_INTERVAL=3000

# 日誌設定
NODE_ENV=development
LOG_LEVEL=info
